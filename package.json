{
    "name": "my-visualization-mod",
    "version": "1.0.0",
    "type": "module",
"scripts": {
  "build": "chmod +x node_modules/.bin/mods-sdk && npx mods-sdk build",
  "build:dev": "npx mods-sdk build --watch --debug",
  "tsc:dev": "tsc --watch",
  "server": "chmod +x node_modules/.bin/mods-dev-server && npx mods-dev-server --no-open",
  "start": "chmod +x node_modules/.bin/mods-dev-server && npx mods-dev-server --no-open"
},
    "devDependencies": {
      "@spotfire/mods-api": "~1.3.0",
      "@spotfire/mods-dev-server": "^1.1.0",
      "@spotfire/mods-sdk": "^1.0.1",
      "esbuild": "^0.21.4",
      "prettier": "^3.3.0",
      "typescript": "^5.4.5"
    },
    "engines": {
      "node": ">=20.0.0"
    }
  }
  